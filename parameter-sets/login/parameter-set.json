        },
        {
            "name": "odata_version",
            "label": "OData protocol version",
            "type": "SELECT",
            "defaultValue" : "v4",
            "selectChoices": [
                {
                    "value": "v2",
                    "label": "Version 2"
                },
                {
                    "value": "v3",
                    "label": "Version 3"
                },
                {
                    "value": "v4",
                    "label": "Version 4"
                }
            ]
        },
        {
            "name": "odata_username",
            "label": "Username",
            "type": "STRING",
            "description": "",
