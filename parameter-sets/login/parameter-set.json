{
    "meta": {
        "label": "OData login",
        "description": "",
        "icon": "icon-list"
    },
    "defaultDefinableInline": true,
    "defaultDefinableAtProjectLevel": true,
    "pluginParams": [],
    "params": [
        {
            "name": "odata_instance",
            "label": "Resource",
            "type": "STRING",
